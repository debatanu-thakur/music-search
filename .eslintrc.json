    "env": {
    "browser": true,
    "es6": true,
    "jasmine": true,
    "node": true
  },
  "plugins": [
    "jasmine"
  ],
  "globals": {
    "angular": false,
    "inject": true,
    "jQuery": true,
    "$": true
